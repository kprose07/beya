{
  "expo": {
    "name": "T.A.R",
    "slug": "t-a-r",
    "version": "1.0.0",
    "description":"Teaching You, Your Way",
    "orientation": "portrait",
    "icon": "./assets/images/TAR.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/TAR.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/TAR.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/TARW.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#2B303A"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
